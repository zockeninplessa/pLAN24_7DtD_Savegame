  <!-- streettile
	mintiles, maxtiles limits the count to that range for the entire world. mintiles 0 does not increase chance to get the tile
	maxdensity sets the prefab budget for the tile (Score in previewer)
	-->
  <!--
	<streettile name="rwg_tile_downtown_intersection_02">
		<property name="mintiles" value="5"/>
		<property name="maxtiles" value="30"/>
	</streettile>
	-->
  <!-- limit these as they block traders from spawning -->
  <streettile name="rwg_tile_gateway_straight3">
    <property name="mintiles" value="0" />
    <property name="maxtiles" value="1" />
  </streettile>
  <streettile name="rwg_tile_gateway_straight3a">
    <property name="mintiles" value="0" />
    <property name="maxtiles" value="1" />
  </streettile>
